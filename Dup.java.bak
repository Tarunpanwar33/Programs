class Dup 
{
	public static void main(String[] args) 
	{
		int arr[]={1,2,3,4,5,2,4};

         for (int i=0;i<arr.length ;i++ )
		{
			for (int j=1+i;j<arr.length ;j++ )
			{
				if (arr[j]==arr[i])
				{
                  System.out.println("The Duplicate elements are "+arr[j]);
				}
			}
		}
		
	}
}
